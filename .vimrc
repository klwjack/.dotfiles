set number

set ignorecase
set smartcase

set incsearch
set hlsearch

set expandtab
set smarttab
set shiftwidth=4
set softtabstop=4
set autoindent

filetype on
filetype plugin on
set ofu=syntaxcomplete#Complete
syntax enable

set modeline
set ls=2

set colorcolumn=80

set cul

set t_Co=256

" solarized settings
set background=dark
colorscheme solarized

" zenburn settings
"colorscheme zenburn

